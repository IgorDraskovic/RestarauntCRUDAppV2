<%= button_to "Back to Orders", orders_path, method: :get, class: 'waves-effect waves-light red btn-large' %></li>
<%= button_to "Admin Menu Options ", foods_path, method: :get, class: 'waves-effect waves-light red btn-large' %></li>


<div class="row">

  <%= form_for @food do |f| %>
  <%= f.label :cuisine %>
  <%= f.text_field :cuisine %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :cost %>
  <%= f.number_field :cost %><br>
  <%= f.submit %>
  <% end %>

</div>
